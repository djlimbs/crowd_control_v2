<div class="container">
	<div class="clearfix">
		<span class="pull-right main-label">Crowd Control</span>
		<ul class="nav nav-tabs">
            {{#each tabs itemController="tab"}}
                <li {{bind-attr class="active"}} {{action "clickSelectTab"}}><a href="#">{{name}}</a></li>
            {{/each}}
	    </ul>
	</div>
	{{render "tab" selectedTab}}
	<div class="footer">
		<p>&copy; Erika 2014</p>
	</div>
</div>